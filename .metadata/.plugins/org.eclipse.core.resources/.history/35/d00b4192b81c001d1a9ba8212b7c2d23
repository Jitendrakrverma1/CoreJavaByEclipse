package Jitendra;

public class String_Comparision {

	public static void main(String[] args) {
		String s1 = "Lion";   // String Literal
		String s2 = "Lion";
		String s3 = new String("Lion");  // using new keyword
		
		// using == operator
	if(s1==s2) 
	     System.out.print("Both are equal..");
	else
		System.out.print("Both are not equal..");
//	output:= Both are  equal
	// using equals()
	if(s1.equals(s3))
		System.out.print("yes");
	else
		System.out.print("No");
	}
}
