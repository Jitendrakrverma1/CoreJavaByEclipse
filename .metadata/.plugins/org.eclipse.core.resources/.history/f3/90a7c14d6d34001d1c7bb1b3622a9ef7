package maccess;
import java.util.*;
public class DemoSet2 
{
  //@SuppressWarnings("removal")
  public static void main(String[] args)
    {
	 Scanner s = new Scanner(System.in);
	 Set<Integer> ob = null;
	 System.out.println("Number Of Oprerations.........");
	 System.out.println("1.Hashset\n2.LinkedHashset\n3.TreeSet");
	 System.out.println("Enter the Choice:");
	 switch(s.nextInt())
	 {
	   case 1:
		   System.out.println("User Choose HashSet........");
//		   ob = HashSet<Integer>();
		   ob = new HashSet<Integer>();
		   break;
	   case 2:
		   System.out.println("User Choosse LinkedHashset.");
//		   ob = LinkedHashSet<Integer>();
		   ob = new LinkedHashSet<Integer>();
		   break;
	   case 3:
		   System.out.println("User Chhoose treeSet...");
//		   ob = TreeSet<Integer>();
		   ob = new HashSet<Integer>();
		   break;
	   default:
		   System.out.println("Invalid Coice.......");
		   System.exit(0);
	 }
	 System.out.println("Enter the number of element to be added:");
	 int n = s.nextInt();
	 System.out.println("Enter"+n+" Integer Objects:");
	 for(int i=1; i<=n; i++)
	 {
		 ob.addAll(s.nextInt());
	 }
	 System.out.println("=====Display from set<E>===");
	 System.out.println(ob.toString());
	 
	 s.close();
  }
}
Scanner s = new Scanner(System.in);
Set<Integer> ob = null;
System.out.println("=======chocie=====");
System.out.println("1.HashSet\n2.LinkedHashSet\n3.TreeSet");
System.out.println("Enter the choice:");
switch(s.nextInt())
{
  case 1:
  	System.out.println("User Choose HashSet...........\n");
      ob = new HashSet<Integer>();
  	break;
  case 2:
  	System.out.println("User Choose LinkedHashSet.........\n");
  	ob = new LinkedHashSet<Integer>();
  	break;
  case 3:
  	System.out.println("User Choose TreeSet........\n");
  	ob = new TreeSet<Integer>();
  	break;
  default:
  	System.out.println("Invalid Choice...........");
  	System.exit(0);
}  // end of switch
System.out.println("Enter the number of element to be added:");
int n = s.nextInt();
System.out.println("Enter"+n+" Integer Objects:");
for(int i=1; i<=n; i++)
{
	  ob.add(new Integer(s.nextInt()));
}// end of loop
System.out.println("======Display from set<E>====");
System.out.println(ob.toString());
s.close();
break;
}
