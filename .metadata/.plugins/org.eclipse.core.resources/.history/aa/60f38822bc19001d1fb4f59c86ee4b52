
                    // Print A to Z Alphabet    
package Jitendra;
public class A_to_Z_print_Alphabet {
	public static void main(String[] args) {
	    for(char i='A'; i<='Z'; i++)
	    {
	    	System.out.print(i+" ");
	    }
	    for(int i=65; i<=90; i++)
	    {
	    	System.out.printf("%c",i);
	    	
	    }
	    int j=65;
	    while(j<=90) {
	    	System.out.printf("%c",j);
	    }
	}

}
