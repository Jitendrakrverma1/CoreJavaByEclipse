
          // Different way to print array element
package Jitendra;
public class Print_Array_Element {
	public static void main(String[] args) {
		int a[] = new int[5];
		a[0] = 11;
		a[1]=  12;
		a[2] = 13;
		a[3] = 14;
		a[4] = 15;
		for(int i=0; i<=4; i++) {
			System.out.print(a[i]+" ");
		}
	}

}
