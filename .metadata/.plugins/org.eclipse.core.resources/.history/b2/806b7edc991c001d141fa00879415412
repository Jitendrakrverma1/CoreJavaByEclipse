
                  // Star Pattern Program1
package Jitendra;

public class Pattern1 {
	public static void main(String[] args) {
		int i,j;
		for( i=1; i<=4; i++) {
			for( j=1; j<=i; j++) {
				System.out.print("*");
			}
			System.out.print("\n");
		}
		
// output:= 
//		*
//		**
//		***
//		****		
		// Star pattern 2
		for(i=1; i<=4; i++) {
			for(j=5; j>=1; j--) {
				System.out.print("*");
			}
			System.out.print("\n");
		}
	}
}
