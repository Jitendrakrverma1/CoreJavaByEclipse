
package jitendra;
public class ChildClass extends ParentClass
 {
  public int b = 20;
  public void m2() 
  {
	System.out.print("====CClass instance method====");  
	System.out.print("The value of b:"+b);
  }
  // instance block for CClass
  {
	  System.out.print("=Instance block for CClass==");
	  System.out.print("The value of b:"+b);
  }
}
