
           // Print diamond symbol
package Jitendra;
public class Print_Diamond_symbol {
	public static void main(String[] args) {
		int i,j;
		for(i=1; i<=5; i++) {
			for(j=5; j>i; j--) {
				System.out.print(" ");
			}
			for(int k=1; k<=(2*i-1); k++) {
				System.out.print("*");
			}
			System.out.print("\n");
		}
		
		for(i=5; i>=1; i--) {
			for(j=5; j>i; j--) {
				System.out.print(" ");
			}
			for(int k=1; k<=(2*i-1); k++) {
				System.out.print("*");
			}
			System.out.print("\n");
		}
	}

}
