package test;
public class EmpRegistration {
  public void register(java.util.Scanner s , Employeee e) {
	  //employee object below
	  System.out.println("Enter the  EmpId:");
	  e.eId  = s.nextLine();
	  System.out.println("Enter the EmpName:");
	  e.eName = s.nextLine();
	  System.out.println("Enter the EmpDesg:");
	  e.eDesg = s.nextLine();
	  //contact object below
	  System.out.println("Enter the mailId:");
	  e.c.mailId = s.nextLine();
	  System.out.println("Enter the phNo:");
	  e.c.phNo = s.nextLong();
	  // Salary object below
	  System.out.println("Enter the bSal:");
	  e.sl.bSal = s.nextDouble();
	  System.out.println("Enter the totsal:");
	  e.sl.totsal = s.nextDouble();
  }
}
