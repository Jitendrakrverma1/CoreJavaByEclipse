package Jitendra;
import java.util.Scanner;
public class Odd_Num_in_Given_Range 
 {
	public static void main(String[] args) {
       int n;
       Scanner s = new Scanner(System.in);
       System.out.println("Enter the numbr:");
       n = s.nextInt();
       for(int i=1; i<=n; i+=2) 
       {
    	   System.out.print("Odd number"+ i +" " );
       }
       }

}
