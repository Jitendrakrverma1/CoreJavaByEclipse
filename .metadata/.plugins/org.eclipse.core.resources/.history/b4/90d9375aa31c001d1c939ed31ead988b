
        // Star Pattern 3
package Jitendra;
public class Pattern3 {
	public static void main(String[] args) {
		int i, j;
		for(i=1; i<=5; i++)   //row
		{
			for(j=i; j<5; j++)  // space
			{
				System.out.print(".");
			}
			for(int k=1;k<=i;k++)   // star
			{
				System.out.print("* ");
			}
			System.out.print("\n");
		}
//output:=
//	     * 
//	    * * 
//	   * * * 
//	  * * * * 
//	 * * * * * 
	}
}
