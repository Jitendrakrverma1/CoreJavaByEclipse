
           // Number Pattern
package Jitendra;
public class Pattern_Number4 {
	public static void main(String[] args) {
		int i,j;
		for(i=1;i<=5;i++) {
			for(j=1;j<=i;j++) {
				System.out.print(j);
			}
			System.out.print("\n");
		}
//output:=
//		1
//		12
//		123
//		1234
//		12345
		
		// star pattern 2
		for(i=1;i<=5;i++) {       // row
			for(j=1;j<=i;j++) {
				System.out.print(i);   // column
			}
			System.out.print("\n");
		}
	}

}
