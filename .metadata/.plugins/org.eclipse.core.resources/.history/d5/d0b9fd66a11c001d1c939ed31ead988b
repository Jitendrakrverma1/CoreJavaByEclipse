
          // star patter 2
package Jitendra;
public class Pattern2 {
	public static void main(String[] args) {
	  int i,j;
	  for(i=1; i<=5; i++) {        //rows
		  for(j=1; j<=5;j++) {      // column
			  System.out.print("*");
		  }
		  System.out.print("\n");
	  }
//output:
//	  *****
//	  *****
//	  *****
//	  *****
//	  *****
	  // Star pattern 2
	  for(i=1; i<=5; i++) {        //rows
		  for(j=1; j<=5;j++) {
			  if(i==1||i==3||j==1||j==3) {
				  System.out.print("*");  
			  }
			  else {
				  System.out.print(" ");
			  }
		  }
		  System.out.print("\n");
	  }
	}
}
