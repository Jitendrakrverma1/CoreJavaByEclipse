package Jitendra;
import java.util.Scanner;
public class Multiplication_Table {
	public static void main(String[] args) {
	 int n;
	 Scanner s = new Scanner(System.in);
	 System.out.print("Enter the value of n:");
	 n = s.nextInt();
	 for(int i=1; i<=n; i++) {
		  n = n*i;
	 }
	 System.out.println("Multiplication table......");
	 System.out.print(n);
	 s.close();
	}

}
